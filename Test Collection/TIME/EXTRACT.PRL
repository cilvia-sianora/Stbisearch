#!/usr/local/bin/perl
       open(data,"time.all");
       open(out,">doc-number");
       $line = <data>;
      while ($line ne "")
      {
        @aray = split(/\ +/,$line);
        $count = @aray;
        $i = 0;
        while ($i <= $count)
         {
           if ($aray[$i] eq "*TEXT")
             {
               $j = $i+1;
               print out ("$aray[$j]\n");
              }
            $i = $i + 1;
          }
       $line = <data>;
    }

        
